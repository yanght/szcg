@{
    Layout = null;
}
<title>Form Elements - Ace Admin</title>
<link rel="stylesheet" href="/content/assets/css/jquery-ui.custom.min.css" />
<link rel="stylesheet" href="/content/assets/css/chosen.css" />
<link rel="stylesheet" href="/content/assets/css/datepicker.css" />
<link rel="stylesheet" href="/content/assets/css/bootstrap-timepicker.css" />
<link rel="stylesheet" href="/content/assets/css/daterangepicker.css" />
<link rel="stylesheet" href="/content/assets/css/bootstrap-datetimepicker.css" />
<link rel="stylesheet" href="/content/assets/css/colorpicker.css" />

<div class="row">
    <div class="col-xs-12">
        <!-- PAGE CONTENT BEGINS -->
        <!--查询条件-->
        <div class="widget-box widget-color-blue">
            <div class="widget-header">
                <h5 class="widget-title">查询条件</h5>
            </div>
            <div class="widget-body">
                <div class="widget-main">
                    <form class="form-inline">

                        <label>问题来源：</label>
                        <select class="form-control" name="ProbSource" id="ProbSource">
                            <option value="">全部</option>
                            <option value="1">监督员上报</option>
                            <option value="2">领导交办</option>
                            <option value="3">网站举报</option>
                            <option value="6">信访举报</option>
                            <option value="7">媒体举报</option>
                            <option value="9">其他举报</option>
                            <option value="11">电话举报</option>
                            <option value="10">监督员快速上报</option>
                            <option value="15">微信举报</option>
                            <option value="16">监控抓拍</option>
                        </select>

                        <label>问题分类：</label>
                        <select class="form-control" name="ProbClass" id="ProbClass">
                            <option value="">全部</option>
                            <option value="0">部件</option>
                            <option value="1">事件</option>
                        </select>

                        <label>大类：</label>
                        <select class="form-control" name="BigClass" id="BigClass">
                            <option value="">全部</option>
                        </select>

                        <label>小类：</label>
                        <select class="form-control" name="SmallClass" id="SmallClass">
                            <option value="">全部</option>
                        </select>

                        <label>区域：</label>
                        <select class="form-control" name="AreaId" id="AreaId">
                            <option value="">全部</option>
                        </select>

                        <label>街道：</label>
                        <select class="form-control" name="StreetId" id="StreetId">
                            <option value="">全部</option>
                        </select>

                        <label>社区：</label>
                        <select class="form-control" name="SquareId" id="SquareId">
                            <option value="">全部</option>
                        </select>

                        <label>删除：</label>
                        <select class="form-control" name="DeleteState" id="DeleteState">
                            <option value="">全部</option>
                            <option value="0">正常</option>
                            <option value="1">已删除</option>
                            <option value="2">已归档</option>
                            <option value="3">回退案卷</option>
                        </select>

                       

                        <br /><br />

                        <label>受理员：</label>
                        <select class="form-control" name="Telephonist" id="Telephonist">
                            <option value="">全部</option>
                        </select>

                        <label>巡查员：</label>
                        <select class="form-control" name="CollCode" id="CollCode">
                            <option value="">全部</option>
                        </select>

                        <label>类型：</label>
                        <select class="form-control" name="ProjectKind" id="ProjectKind">
                            <option value="">全部</option>
                            <option value="0">举报一般案卷</option>
                            <option value="1">举报重大案卷</option>
                            <option value="2">督办案卷</option>
                        </select>

                        <label>职能部门：</label>
                        <select class="form-control" name="TargetDepartCode" id="TargetDepartCode">
                            <option value="">全部</option>
                        </select>

                        <label>状态：</label>
                        <select class="form-control" name="ProjectState" id="ProjectState">
                            <option value="">全部</option>
                            <option value="1">已立案(全部)</option>
                            <option value="2">已派遣(全部)</option>
                            <option value="21">已派遣(未处理)</option>
                            <option value="31">已处理(未结案)</option>
                            <option value="9">已结案(全部)</option>
                        </select>

                        <br /><br />

                        <label>办理时间：</label>
                        <div class="input-daterange input-group" data-date-format="yyyy-mm-dd">
                            <input type="text" class="input-sm form-control" name="DoStartTime" id="DoStartTime">
                            <span class="input-group-addon">
                                至
                            </span>
                            <input type="text" class="input-sm form-control" name="DoEndTime" id="DoEndTime" value="2016-12-1">
                        </div>

                        <label>发生地址：</label>
                        <input type="text" class="form-control" placeholder="" name="Address" id="Address">

                        <label>上报时间：</label>
                        <div class="input-daterange input-group" data-date-format="yyyy-mm-dd">
                            <input type="text" class="input-sm form-control" name="StartTime">
                            <span class="input-group-addon">
                                至
                            </span>
                            <input type="text" class="input-sm form-control" name="EndTime" value="2016-12-1">
                        </div>

                        <button type="button" class="btn btn-info btn-sm" id="query">
                            查询
                        </button>

                        <button type="reset" class="btn btn-info btn-sm">
                            重置
                        </button>

                    </form>
                </div>
            </div>
        </div>
        <!--案卷列表-->
        <div class="widget-box widget-color-blue">
            <div class="widget-header">
                <h5 class="widget-title">查询箱案卷列表</h5>
                <div class="widget-toolbar">
                    <a href="#" data-action="collapse">
                        <i class="1 ace-icon fa fa-chevron-up bigger-125"></i>
                    </a>
                </div>
                <div class="widget-toolbar no-border">

                    <button class="btn btn-xs bigger btn-yellow dropdown-toggle" data-toggle="dropdown">
                        操作
                        <i class="ace-icon fa fa-chevron-down icon-on-right"></i>
                    </button>
                    <ul class="dropdown-menu dropdown-yellow dropdown-menu-right dropdown-caret dropdown-close">
                        <li>
                            <a href="#">导出电子表格</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="widget-body">
                <div class="widget-main">
                    <table id="projectcdlistTB" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>计时</th>
                                <th>锁定</th>
                                <th>督办</th>
                                <th>来源</th>
                                <th>状态</th>
                                <th> 案卷号</th>
                                <th>接听人</th>
                                <th>分类</th>
                                <th>大类</th>
                                <th>小类</th>
                                <th>案卷受理时间</th>
                                <th>当前步骤派发时间</th>

                                <th>街道</th>
                                <th>社区</th>
                                <th>描述</th>
                                <th>操作</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
        <!-- PAGE CONTENT ENDS -->
    </div><!-- /.col -->
</div><!-- /.row -->
